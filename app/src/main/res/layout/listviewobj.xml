<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/listobj">

    <TextView
        android:id="@+id/name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="11dp"
        android:padding="6dp"
        android:text="TextView"
        android:textSize="32dp"
        android:layout_marginStart="9dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />



    <TextView
        android:id="@+id/frequencytitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Frequency: "
        android:layout_marginLeft="50dp"
        android:layout_below="@+id/durationtitle"
        android:layout_alignEnd="@+id/durationtitle" />

    <TextView
        android:id="@+id/frequency"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:layout_marginLeft="50dp"
        android:layout_below="@+id/duration"
        android:layout_alignEnd="@+id/duration" />

    <TextView
        android:id="@+id/durationtitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Duration: "
        android:layout_marginLeft="50dp"
        android:layout_below="@+id/intensitytitle"
        android:layout_alignEnd="@+id/intensitytitle" />

    <TextView
        android:id="@+id/duration"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:layout_marginLeft="50dp"
        android:layout_marginStart="26dp"
        android:layout_alignBaseline="@+id/durationtitle"
        android:layout_alignBottom="@+id/durationtitle"
        android:layout_toEndOf="@+id/frequencytitle" />

    <TextView
        android:id="@+id/intensitytitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="0.56"
        android:text="Intensity:"
        android:layout_marginLeft="50dp"
        android:layout_alignTop="@+id/name"
        android:layout_toEndOf="@+id/name"
        android:layout_marginStart="26dp" />

    <TextView
        android:id="@+id/intensity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:layout_marginLeft="50dp"
        android:layout_above="@+id/duration"
        android:layout_alignEnd="@+id/duration" />


</RelativeLayout>