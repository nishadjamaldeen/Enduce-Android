<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ff8300"
    tools:context="com.nishadjamaldeen.enducev01.MainActivity">


    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="78dp"
        android:text="WELCOME"
        android:textColor="@android:color/white"
        android:textStyle="bold"
        android:textSize="44dp"/>

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/horizline"
        android:layout_marginBottom="40dp"
        android:layout_alignBottom="@+id/imageView2"
        android:layout_alignParentStart="true" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/VertLine"
        android:scaleY="0.4"
        android:scaleX="0.5"
        android:layout_marginTop="62dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="39dp"
        android:scaleX="0.9"
        android:scaleY="0.9"
        app:srcCompat="@drawable/iconschange"
        android:layout_below="@+id/textView"
        android:layout_alignEnd="@+id/imageView2" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="45dp"
        android:scaleX="0.9"
        android:scaleY="0.9"
        app:srcCompat="@drawable/iconsnew"
        android:layout_centerVertical="true"
        android:layout_alignStart="@+id/imageView2" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="71dp"
        android:scaleX="0.8"
        android:scaleY="0.8"
        app:srcCompat="@drawable/iconsAnalytics"
        android:layout_alignTop="@+id/imageView"
        android:layout_alignParentStart="true" />

</RelativeLayout>
